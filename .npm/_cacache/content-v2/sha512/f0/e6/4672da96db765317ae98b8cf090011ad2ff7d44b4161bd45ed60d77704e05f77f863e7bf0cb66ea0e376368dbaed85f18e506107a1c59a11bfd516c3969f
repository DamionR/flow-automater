n    );\n\n    const textfield = root.createComponent(TextField, {\n      label: i18n.translate('gift_note.label'),\n      value: metafield?.value ?? '',\n      onChange(value) {\n        textfield.updateProps({value});\n\n        applyMetafieldChange({\n          namespace: 'custom',\n          key: 'gift_note',\n          value,\n          valueType: 'string',\n        });\n      },\n    });\n\n    root.appendChild(textfield);\n  },\n);\n```\n","exports":{".":{"types":"./build/ts/index.d.ts","esnext":"./index.esnext","import":"./index.mjs","require":"./index.js"},"./checkout":{"types":"./build/ts/surfaces/checkout.d.ts","esnext":"./checkout.esnext","import":"./checkout.mjs","require":"./checkout.js"}},"gitHead":"382eea4bef25a436fa7dfee89df318490a81f59b","_npmUser":{"name":"shopify-dep","email":"npmjs+shopify-dep@shopify.com"},"_npmVersion":"8.1.2","description":"This package contains the public type definitions and utilities needed to create a Shopify UI extension. This is a generalized package that is intended to be the long-term home of the surface-specific UI extension packages, like [`@shopify/checkout-ui-ext","directories":{},"sideEffects":false,"_nodeVersion":"16.13.1","dependencies":{"@remote-ui/core":"2.1.x","@remote-ui/async-subscription":"^2.1.12"},"eslintConfig":{"rules":{"@shopify/strict-component-boundaries":"off"}},"publishConfig":{"access":"public","@shopify:registry":"https://registry.npmjs.org/"},"typesVersions":{"*":{"checkout":["./build/ts/surfaces/checkout.d.ts"]}},"_hasShrinkwrap":false,"readmeFilename":"README.md","_npmOperationalInternal":{"tmp":"tmp/ui-extensions_0.0.0-unstable-20230118190150_1674068518941_0.18086001112089423","host":"s3://npm-registry-packages"}},"0.0.0-unstable-20230118192216":{"name":"@shopify/ui-extensions","version":"0.0.0-unstable-20230118192216","license":"MIT","_id":"@shopify/ui-extensions@0.0.0-unstable-20230118192216","maintainers":[{"name":"michaelarsenault","email":"michael.arsenault@shopify.com"},{"name":"wcand