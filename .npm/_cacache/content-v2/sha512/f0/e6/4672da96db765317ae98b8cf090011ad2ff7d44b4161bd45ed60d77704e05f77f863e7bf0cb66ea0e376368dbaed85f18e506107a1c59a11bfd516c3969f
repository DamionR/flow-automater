afields, applyMetafieldChange}) => {\n    const metafield = metafields.current.find(\n      (metafield) =>\n        metafield.namespace === 'custom' && metafield.key === 'gift_note',\n    );\n\n    const textfield = root.createComponent(TextField, {\n      label: i18n.translate('gift_note.label'),\n      value: metafield?.value ?? '',\n      onChange(value) {\n        textfield.updateProps({value});\n\n        applyMetafieldChange({\n          namespace: 'custom',\n          key: 'gift_note',\n          value,\n          valueType: 'string',\n        });\n      },\n    });\n\n    root.appendChild(textfield);\n  },\n);\n```\n","exports":{".":{"types":"./build/ts/index.d.ts","esnext":"./index.esnext","import":"./index.mjs","require":"./index.js"},"./admin":{"types":"./build/ts/surfaces/admin.d.ts","esnext":"./build/esnext/surfaces/admin.esnext","import":"./build/esm/surfaces/admin.mjs","require":"./build/cjs/surfaces/admin.js"},"./checkout":{"types":"./build/ts/surfaces/checkout.d.ts","esnext":"./build/esnext/surfaces/checkout.esnext","import":"./build/esm/surfaces/checkout.mjs","require":"./build/cjs/surfaces/checkout.js"},"./point-of-sale":{"types":"./build/ts/surfaces/point-of-sale.d.ts","esnext":"./build/esnext/surfaces/point-of-sale.esnext","import":"./build/esm/surfaces/point-of-sale.mjs","require":"./build/cjs/surfaces/point-of-sale.js"},"./customer-account":{"types":"./build/ts/surfaces/customer-account.d.ts","esnext":"./build/esnext/surfaces/customer-account.esnext","import":"./build/esm/surfaces/customer-account.mjs","require":"./build/cjs/surfaces/customer-account.js"}},"gitHead":"ac077c8becee6f02fbb4fd9f04d029dbf9b838f2","scripts":{"docs:admin":"sh ./docs/surfaces/admin/build-docs.sh","docs:checkout":"sh ./docs/surfaces/checkout/build-docs.sh","gen-docs:admin":"sh ./docs/surfaces/admin/create-doc-files.sh \"admin\"","docs:point-of-sale":"sh ./docs/surfaces/point-of-sale/build-docs.sh"},"_npmUser":{"name":"shopify-dep","email":"npmjs+shopify-dep@shopify.com